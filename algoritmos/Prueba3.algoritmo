Accion Prueba es;

    Ambiente
        numero:entero;
        i:entero;

    Algoritmo

        Escribir("Comenzando");
        numero := 0;
        Para(i:=0 hasta 50, i:=i+2)hacer
            numero := numero +1;
            Si(numero mod 6 == 0)entonces
                Escribir("****",numero)
            sino
                 Escribir(numero);
            Fin_si
        Fin_para

    Fin_Algoritmo

Fin_Accion;